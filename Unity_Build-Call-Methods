//This script is only run in the editor when building the project


#if UNITY_EDITOR
using UnityEditor;
using UnityEditor.Build;

class PrebuildVersionIncrement : IPreprocessBuildWithReport
{
	public int callbackOrder { get { return 0; } }

	public void OnPreprocessBuild (UnityEditor.Build.Reporting.BuildReport buildReport)
	{
  
    //In this example, the function of an instance is called.
		Collectables.LoadCollectables();
	}
}
#endif


